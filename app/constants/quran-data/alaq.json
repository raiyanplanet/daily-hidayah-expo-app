[
  {"number":1,"arabic":"ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ","translation":"Read, ˹O Prophet,˺ in the Name of your Lord Who created—","transliteration":"Iqra' bismi rabbika allathee khalaq","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":2,"arabic":"خَلَقَ ٱلْإِنسَـٰنَ مِنْ عَلَقٍۢ","translation":"created humans from a clinging clot.","transliteration":"Khalaqa al-insana min 'alaq","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":3,"arabic":"ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ","translation":"Read! And your Lord is the Most Generous,","transliteration":"Iqra' wa rabbuka al-akram","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":4,"arabic":"ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ","translation":"Who taught by the pen—","transliteration":"Allathee 'allama bil-qalam","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":5,"arabic":"عَلَّمَ ٱلْإِنسَـٰنَ مَا لَمْ يَعْلَمْ","translation":"taught humanity what they knew not.","transliteration":"Allama al-insana ma lam ya'lam","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":6,"arabic":"كَلَّا إِنَّ ٱلْإِنسَـٰنَ لَيَطْغَىٰٓ","translation":"Most certainly, man transgresses","transliteration":"Kalla inna al-insana layatgha","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":7,"arabic":"أَن رَّءَاهُ ٱسْتَغْنَىٰ","translation":"once he thinks he is self-sufficient.","transliteration":"An ra'ahu istaghna","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":8,"arabic":"إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰ","translation":"˹But˺ surely to your Lord is the return.","transliteration":"Inna ila rabbika ar-ruj'a","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":9,"arabic":"أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ","translation":"Have you seen the one who prevents","transliteration":"Ara'ayta allathee yanha","juz":30,"page":597,"ruku":1,"sajda":false},
  {"number":10,"arabic":"عَبْدًا إِذَا صَلَّىٰ","translation":"a servant when he prays?","transliteration":"'Abdan itha salla","juz":30,"page":597,"ruku":1,"sajda":false}
